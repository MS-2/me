# With WebAssembly Streaming.
<IfModule mod_mime.c>
  AddEncoding gzip .unityweb
  AddEncoding gzip .wasm
  AddType application/wasm .wasm
</IfModule>

<filesMatch ".(hash|js|json)$">
Header set Cache-Control "max-age=0, must-revalidate"
</filesMatch>

